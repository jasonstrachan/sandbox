{
  "name": "sandbox",
  "version": "1.0.0",
  "description": "nothing is everything",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:clean": "rm -rf node_modules/.vite && vite --force",
    "build": "vite build",
    "preview": "vite preview",
    "preview:prod": "npm run build && npm run preview",
    "test": "node --test",
    "test:watch": "node --test --watch",
    "prepare": "husky",
    "lint": "npm run lint:js && npm run lint:wgsl",
    "lint:js": "eslint . --max-warnings=0",
    "lint:wgsl": "node scripts/wgsl-lint.mjs",
    "typecheck": "tsc --noEmit --pretty false",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonstrachan/sandbox.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jasonstrachan/sandbox/issues"
  },
  "homepage": "https://github.com/jasonstrachan/sandbox#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.0",
    "@webgpu/types": "^0.1.66",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "typescript": "^5.9.3",
    "vite": "^7.1.6"
  }
}
